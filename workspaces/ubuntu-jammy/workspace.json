{
  "description": "Customized Terminal with nchat compilation",
  "docker_registry": "https://index.docker.io/v1/",
  "image_src": "nchat.png",
  "name": "chriswilson2005/nchat-workspace:1.15.0",
  "friendly_name": "Terminal-nchat",
  "cores": 2,
  "memory": 4096,
  "gpu_count": 0,
  "cpu_allocation_method": "Inherit",
  "run_config": { 
    "hostname": "kasm-nchat",  
    "environment": {
      "TERMINAL_ARGS": "--fullscreen --hide-borders --hide-menubar --zoom=-1 --hide-scrollbar"
    }
  },
  "categories": [
    "Terminal",
    "Productivity"
  ],
  "architecture": [
    "amd64"
  ],
  "compatibility": [
    "1.15.x",
    "1.14.x"
  ],
  "uncompressed_size_mb": 1720
}
